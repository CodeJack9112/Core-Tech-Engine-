{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: Configure (default)",
      "type": "shell",
      "command": "cmake --preset default",
      "group": "build"
    },
    {
      "label": "CMake: Build (default)",
      "type": "shell",
      "command": "cmake --build --preset default",
      "group": "build"
    },
    {
      "label": "CMake: Configure (release)",
      "type": "shell",
      "command": "cmake --preset release",
      "group": "build"
    },
    {
      "label": "CMake: Build (release)",
      "type": "shell",
      "command": "cmake --build --preset release",
      "group": "build"
    }
  ]
}